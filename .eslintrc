{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "react-app"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error",
    "react/prop-types": 0,
    "import/no-anonymous-default-export": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
